<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Exceptions Test Suite" verbose="1">

	<test name="ExceptionTest">
		<classes>
			<class name="com.herokuapp.theinternet.ExceptionsTests">
				<methods>
					<exclude name="notVisibleTest" />
					<exclude name="timeoutExceptionTest" />
					<exclude name="noSuchElementExceptionTest" />
					<exclude name="StaleElementReferenceException" />
					<include name="disabledElementTest" />
				</methods>
			</class>
		</classes>
	</test>
</suite>
